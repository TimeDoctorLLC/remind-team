  "scripts": {
    "start": "node app.js",
    
    "test": "mocha --bail \"storage/test/!(fixtures.js)\" \"bot/test/!(bot.js)\"",
    "integration-tests": "mocha --bail web/test/index.js || true",
    
    "build-emails": "cd emails && npm run build",
        
